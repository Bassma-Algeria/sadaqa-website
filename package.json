{
  "name": "sadaqa-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "start:dev": "next dev -p 3000",
    "start:prod": "next start -p 80",
    "lint:check": "next lint",
    "lint:fix": "next lint --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "docker:dev": "docker-compose -f docker/dev/docker-compose.dev.all.yml up",
    "docker:dev:min": "docker-compose -f docker/dev/docker-compose.dev.min.yml up",
    "docker:prod": "docker-compose -f docker/prod/docker-compose.prod.yml up",
    "docker:backend:pull": "docker-compose -f docker/dev/docker-compose.dev.yml build backend"
  },
  "dependencies": {
    "@next/font": "13.1.5",
    "next": "13.1.5",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@swc/jest": "^0.2.24",
    "@types/jest": "^29.4.0",
    "@types/node": "18.11.18",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "eslint": "8.32.0",
    "eslint-config-next": "13.1.5",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.4.1",
    "prettier": "2.8.3",
    "sass": "^1.57.1",
    "typescript": "4.9.4"
  },
  "engines": {
    "node": "18.13.0",
    "npm": "8.19.3"
  }
}
